{
  "name": "camera-stream-for-alarm",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"npm run electron\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "vite": "^6.3.5"
  },
  "build": {
    "files": [
      "dist-electron/**/*",
      "main.js"
    ],
    "directories": {
      "output": "camera-app2"
    }
  }
}